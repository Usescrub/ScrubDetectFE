    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error",
      "space-in-parens": [
        "error",
        "never"
      ],
      "no-multiple-empty-lines": "warn",
      "no-console": "warn",
      "no-use-before-define": "error",
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "semi": [
        "warn",
        "never"
      ],
      "keyword-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "no-trailing-spaces": "error",
      "no-multi-str": "off",
      "import/no-duplicates": "error",
      "quotes": [
        "error",
        "single"
      ]
    }
  }
